{
  "vendors": {
    "alpinejs": {
      "js": {
        "from": "./node_modules/alpinejs/dist/alpine.js",
        "to": "./public/assets/js/vendor/alpine.js"
      }
    },
    "chart.js": {
      "js": {
        "from": "./node_modules/chart.js/dist/Chart.bundle.min.js",
        "to": "./public/assets/js/vendor/Chart.bundle.min.js"
      },
      "css": {
        "from": "./node_modules/chart.js/dist/Chart.min.css",
        "to": "./public/assets/css/vendor/Chart.min.css"
      }
    }
  }
}
